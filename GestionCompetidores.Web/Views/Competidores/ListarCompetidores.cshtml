@using GestionCompetidores.Data.EF
@model List<Competidor>


<h1>Listar de Competidores</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                Nombre Competidor
            </th>
            <th>
                Deporte
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (Competidor competidor in Model) {
        <tr>
            <td>
                    @competidor.NombreCompetidor
            </td>
            <td>
                    @competidor.IdDeporteNavigation.NombreDeporte
            </td>
            
        </tr>
}
    </tbody>
</table>
